generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  clerk_id      String?  @unique
  username      String?  @unique
  email         String   @unique
  fullName      String?
  phoneNumber   String?
}
